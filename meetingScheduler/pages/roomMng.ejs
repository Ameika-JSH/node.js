<div class='row inputDataDiv'>
	<div class='col-lg-10 col-12'>
		<div class = "row">
			<div class="input-group input-group-sm mb-3 col-lg-6 col-12">
			  <div class="input-group-prepend">
				<span class="input-group-text" id="inputGroup-sizing-sm">사무실 이름</span>
			  </div>
			  <input type="text" id = "inputOffice" name = "사무실 이름" class="form-control" aria-label="Small" aria-describedby="inputGroup-sizing-sm">
			</div>
			<div class="input-group input-group-sm mb-3 col-lg-6 col-12">
			  <div class="input-group-prepend">
				<span class="input-group-text" id="inputGroup-sizing-sm">회의실 이름</span>
			  </div>
			  <input type="text" id = "inputRoom" name = "회의실이름" class="form-control" aria-label="Small" aria-describedby="inputGroup-sizing-sm">
			</div>
			<div class="input-group input-group-sm mb-3 col-lg-6 col-12">
			  <div class="input-group-prepend">
				<span class="input-group-text" id="inputGroup-sizing-sm">수용인원</span>
			  </div>
			  <input type="number" id = "inputSize" name = "수용인원" class="form-control" aria-label="Small" aria-describedby="inputGroup-sizing-sm">
			</div>
			<div class="input-group input-group-sm mb-3 col-lg-6 col-12">
			  <div class="input-group-prepend">
				<span class="input-group-text" id="inputGroup-sizing-sm">기타</span>
			  </div> 
			  <input type="text" id = "inputComment" class="form-control" aria-label="Small" aria-describedby="inputGroup-sizing-sm">
			</div>
		</div>
	</div>
	<div class='col-lg-2 col-12'>
		<button class = "btn btn-sm btn-green" id = "btnRoomInsert"><i class="fa fa-save"></i></button>
	</div>
</div>


     
<p id = "tableLabel">회의실 등록 현황</p>
<table class="table table-striped table-bordered" id="roomInfoTable">
	<thead>
		<tr class = 'ar'>
			<th class = 'office'>사무실</th>
			<th class = 'room'>회의실</th>
			<th class = 'size'>수용인원</th>
			<th class = 'comment ar'>기타</th>
			<th  class = "tdButtons"></th>
		</tr>
	</thead>
	<tbody>
	<% for(let i = 0; i < data.length ; i++){ %>
		<tr>
			<td class = 'office' name = "사무실 이름"><%=data[i].OFFICE %></td>
			<td class = 'room' name = "회의실이름" data-toggle="tooltip" data-placement="top" title="<%=data[i].ROOM%>"><%=data[i].ROOM%></td>
			<td class = 'size' name = "수용인원"><%=data[i].SIZE%>명</td>
			<td class = 'comment ar' data-toggle="tooltip" data-placement="top" title="<%=data[i].COMMENT%>"><%=data[i].COMMENT%></td>
			<td class = "tdButtons" data-office = "<%=data[i].OFFICE %>" data-room = "<%=data[i].ROOM%>">
				<button class = "btn btn-sm btn-warning btnRoomUpdate"><i class="fa fa-edit"></i></button>
				<button class = "btn btn-sm btn-danger btnRoomDelete"><i class="fa fa-trash-o"></i></button>
			</td>
		</tr>
	<% } %>		
	</tbody>
</table>